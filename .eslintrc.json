{
  "extends": "airbnb-base",

  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "func-names": ["error", "never"],
    "consistent-return": 0,
    "react/prop-types": 0,
    "prefer-destructuring": 0
  }
}
